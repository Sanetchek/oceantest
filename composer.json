{
    "name": "automattic/oceantest",
    "type": "wordpress-theme",
    "description": "Composer Settings for Ocean Theme",
    "keywords": [
        "WordPress",
        "Themes"
    ],
    "homepage": "https://github.com/Sanetchek/oceantest",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Contributors",
            "homepage": "https://github.com/Sanetchek/oceantest/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=5.6",
        "mpdf/mpdf": "^8.1",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
        "wptrt/wpthemereview": "^0.2.1",
        "php-parallel-lint/php-parallel-lint": "^1.2.0",
        "wp-cli/i18n-command": "^2.2.5"
    },
    "scripts": {
        "lint:php": "@php ./vendor/bin/parallel-lint --exclude .git --exclude vendor .",
        "make-pot": "wp i18n make-pot . languages/oceantest.pot"
    },
    "support": {
        "issues": "https://github.com/Sanetchek/oceantest/issues",
        "source": "https://github.com/Sanetchek/oceantest"
    }
}
